{
  "name": "my-velero",
  "version": "1.3.3-stable-1.15.2",
  "description": "Velero dashboard UI",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "test": "cross-env NODE_ENV=test jest --silent --testTimeout=30000",
    "format": "npx prettier . --write"
  },
  "nodemonConfig": {
    "ext": "js,json,mjs,html,html.twig"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx",
      "jsx",
      "node"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json"
        }
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@kubernetes/client-node|openid-client|oauth4webapi|jose)/)"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "author": "Marouan MOHAMED",
  "license": "Apache-2.0",
  "type": "module",
  "dependencies": {
    "@babel/preset-env": "^7.29.0",
    "@kubernetes/client-node": "^1.4.0",
    "axios": "^1.13.5",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.20.4",
    "cookie-parser": "^1.4.7",
    "cron-validator": "^1.4.0",
    "csrf": "^3.1.0",
    "csurf": "^1.11.0",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "html-entities": "^2.6.0",
    "ldap-authentication": "^2.3.3",
    "oidc-client": "^1.11.5",
    "openid-client": "^6.8.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "prom-client": "^15.1.3",
    "sanitizer": "^0.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twing": "^7.2.2",
    "xss-shield": "^1.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "js-yaml": "^4.1.1",
    "jsdom": "^21.1.2",
    "nodemon": "^3.1.11",
    "prettier": "3.1.0",
    "supertest": "^6.3.4",
    "supertest-session": "^5.0.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
